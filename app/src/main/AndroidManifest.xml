<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.anil.euroleague">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/euroleaguelogo"
            android:label="@string/app_name"
            android:roundIcon="@drawable/euroleaguelogo"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Standings">
        </activity>
        <activity android:name=".Livescore">
        </activity>
        <activity android:name=".Results">
        </activity>
    </application>

</manifest>